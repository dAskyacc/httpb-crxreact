{
    "name": "BAS",
    "manifest_version": 3,
    "version": "0.0.1",
    "options_page": "options.html",
    "background": {
        "service_worker": "background.bundle.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icons/brave-active-128.png"
    },
    "icons": {
        "128": "icons/brave-active-128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "content.styles.css",
                "icons/*.png"
            ],
            "matches": []
        }
    ]
}